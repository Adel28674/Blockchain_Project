<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.blockchain.AccueilPage">
    <BorderPane prefHeight="616.0" prefWidth="1000.0" xmlns:fx="http://javafx.com/fxml/1">
        <left>
            <VBox spacing="10">
                <Button fx:id="btnSidebarAccueil" onAction="#onbtnSidebarAccueilClicked" text="Accueil" />
                <Button fx:id="btnSidebarWalletManager" onAction="#onbtnSidebarWalletManagerClicked" text="Wallet Manager" />
            <Button fx:id="btnSidebaMarket" mnemonicParsing="false" onAction="#onbtnSidebaMarketClicked" text="Market" />
            <Button fx:id="btnSidebaProfile" mnemonicParsing="false" onAction="#onbtnProfileClicked" text="Profil" />
                <!-- Ajouter d'autres boutons si nÃ©cessaire -->
            </VBox>
        </left>
        <center>
            <HBox prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                <children>
                    <VBox alignment="CENTER" prefHeight="400.0" prefWidth="212.0" spacing="20.0">
                        <Label fx:id="WalletValue" text="0">AAAAA</Label>
                  <HBox alignment="CENTER" onMouseClicked="#onBitcoinLabelClicked" prefHeight="20.0" prefWidth="30.0">
                     <children>
                              <Label text="Bitcoin : ">AAAAA</Label>
                        <Label fx:id="bitcoinChange" text="0" />
                     </children>
                  </HBox>
                        <HBox alignment="CENTER" onMouseClicked="#onEthLabelClicked" prefHeight="20.0" prefWidth="30.0">
                     <children>
                              <Label text="Etherum : ">AAAAA</Label>
                        <Label fx:id="EthChange" text="0" />
                     </children>
                  </HBox>
                        <HBox alignment="CENTER" onMouseClicked="#onLtcLabelClicked" prefHeight="20.0" prefWidth="30.0">
                     <children>
                              <Label text="LITECOIN :">AAAAA</Label>
                        <Label fx:id="LtcChange" text="0" />
                     </children>
                  </HBox>
                        <HBox alignment="CENTER" onMouseClicked="#onNeoLabelClicked" prefHeight="20.0" prefWidth="30.0">
                     <children>
                              <Label text="NEO : ">AAAAA</Label>
                        <Label fx:id="neoChange" text="0" />
                     </children>
                  </HBox>
                        <HBox alignment="CENTER" onMouseClicked="#onBnbLabelClicked" prefHeight="20.0" prefWidth="30.0">
                     <children>
                              <Label text="BNB : ">AAAAA</Label>
                        <Label fx:id="bnbChange" text="0" />
                     </children>
                  </HBox>
                    </VBox>
                    <VBox alignment="CENTER" prefHeight="616.0" prefWidth="735.0" spacing="20.0">
                        <padding>
                            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                        </padding>
                        <children>
                     <VBox prefHeight="527.0" prefWidth="560.0">
                        <children>
                              <Button fx:id="btn" mnemonicParsing="false" onAction="#onBtnClickChartInitializer" text="Button" />
                           <ComboBox id="combobox" fx:id="comboBoxWallets" prefWidth="150.0">
                               <items>
                                   <FXCollections fx:factory="observableArrayList">
                                       <String fx:value="Item 1" />
                                       <String fx:value="Item 2" />
                                       <String fx:value="Item 3" />
                                   </FXCollections>
                               </items>
                           </ComboBox>
                                <StackPane fx:id="chartContainer">
                              <children>
                                 <LineChart fx:id="lineChartWallet">
                                   <xAxis>
                                     <CategoryAxis side="BOTTOM" />
                                   </xAxis>
                                   <yAxis>
                                     <NumberAxis side="LEFT" />
                                   </yAxis>
                                 </LineChart>
                              </children></StackPane>
                                  
                        </children>
                     </VBox>

                            <HBox alignment="CENTER" prefHeight="408.0" prefWidth="600.0" spacing="20.0">
                                <VBox fx:id="pieChartContainer" alignment="CENTER" prefHeight="408.0" prefWidth="600.0" spacing="20.0" />

                                <VBox alignment="CENTER" prefHeight="408.0" prefWidth="600.0" spacing="20.0">
                           <children>
                              <PieChart fx:id="pieChartRepartitionIndividuals" />
                           </children></VBox>

                            </HBox>
                        </children>

                    </VBox>
                </children>
            </HBox>
        </center>

    </BorderPane>

</AnchorPane>
